<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tracy.fileexplorer" >
    
    <uses-permission android:name="android.permission.MEDIA_CONTENT_CONTROL" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".FEApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LocaleFileMain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LocaleFileBrowser"
            />
        <activity
            android:name=".LocaleMediaFileBrowser"
            />
        <activity
            android:name=".LocaleFileGallery"
            />
        <activity
            android:name=".fileexplorer"
            />
        <activity
            android:name=".TFileViewer"
            />

    </application>

</manifest>
